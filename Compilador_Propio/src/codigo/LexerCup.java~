            }
            // fall through
          case 72: break;
          case 31:
            { return new Symbol(sym.TKN_BOOLEAN      ,yyline+1 ,yycolumn+1 ,yytext());
            }
            // fall through
          case 73: break;
          case 32:
            { return new Symbol(sym.TKN_CHAR         ,yyline+1 ,yycolumn+1 ,yytext());
