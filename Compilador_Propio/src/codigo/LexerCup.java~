            }
            // fall through
          case 83: break;
          case 42:
            { return new Symbol(sym.TKN_SWITCH       ,yyline+1 ,yycolumn+1 ,yytext());
            }
            // fall through
          case 84: break;
          default:
            zzScanError(ZZ_NO_MATCH);
        }
      }
    }
  }


