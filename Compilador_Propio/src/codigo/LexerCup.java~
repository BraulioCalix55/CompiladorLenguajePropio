            // fall through
          case 69: break;
          case 28:
            { return new Symbol(sym.TKN_FOR          ,yyline+1 ,yycolumn+1 ,yytext());
            }
            // fall through
          case 70: break;
          case 29:
            { return new Symbol(sym.TKN_FUNCION      ,yyline+1 ,yycolumn+1 ,yytext());
